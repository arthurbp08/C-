#define echo 
#define trig

unsigned long tempo;
double distancia;

void setup(){
  pinMode(trig,output);
  pinMode(echo,input);
  
  Serial.begin(9600);
  digitalwrite(trig, LOW);

}
void loop(){
    digitalWrite(trig, HIGH);
    delayMicroseconds(10);
    digitalWrite(trig, LOW);

tempo = pulseIn(echo, HIGH);
distancia = tempo/58  
Serial.println(distancia);

delay(500);

}
